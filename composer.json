{
    "name": "spyrit/light-csv",
    "description": "Spyrit Light CSV reader and writer library",
    "type": "library",
    "keywords": ["csv", "import", "export", "reader", "writer", "parser"],
    "authors": [
        {
            "name": "Charles SANQUER",
            "email": "charles.sanquer@spyrit.net"
        }
    ],
    "license": "LGPL-3.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.3",
        "ext-mbstring": "*"
    },
    "suggest": {
        "ext-iconv": "*"
    },
    "require-dev": {
        "symfony/stopwatch": "2.4.*",
        "fzaninotto/faker": "1.3.*",
        "doctrine/inflector": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Spyrit": "src/" }
    }
